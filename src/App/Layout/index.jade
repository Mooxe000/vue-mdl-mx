//- .image-preloader

.docs-layout(
class='mdl-layout'
class='mdl-js-layout'
)

  
  //- Header
  comp-header(
  :tab-name='routeName'
  )
    
  main.docs-layout-content(
  class='mdl-layout__content'
  class='mdl-color-text--grey-600'
  )
    
    #content.content(
    class='mdl-grid.mdl-grid--no-spacing'
    v-bind:class='routeName'
    )
      .subpageheader(
      class='mdl-cell--12-col'
      ) {{ pageTitle }}
      
      router-view
        
    section.docs-text-styling.download(
    class='mdl-color--grey-800'
    )
      
      a(
      class='mdl-color-text--grey-50'
      ) Download Kit
      
  
  //- Footer
  comp-footer
